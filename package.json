{
  "name": "server",
  "version": "1.6.1",
  "description": "Workly Contacts is a secure, full-featured contact management system built with Node.js, Express, TypeScript, and MongoDB. It provides OAuth authentication, activity tracking, session management, contact labeling, and import/export capabilities with a modern, modular architecture. Designed for scalability and performance, it serves as a production-ready backend for managing personal and professional contact data efficiently.",
  "main": "./dist/server.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.test.ts\"",
    "build": "bash scripts/build.sh",
    "start": "node ./dist/server.js",
    "dev": "nodemon",
    "create:module": "node ./scripts/createModule.js",
    "lint": "eslint \"src/**/*.{ts,js}\" --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "create:test": "node ./scripts/createTestModule.js",
    "docs": "typedoc --entryPointStrategy expand src"
  },
